{% extends 'baseHeaderFooter/header.html.twig' %}

{% block title %}Hello Home{% endblock %}

{% block body %}
<div class="sc-partner-listing-page">
    <div class="container"> 
    <div class="row courses__list ">



        {% for u in User  %}
        {% if ( u.roles[0]=="ROLE_PROF" )%}
        <div class="col-lg-4 col-md-6 col-sm-6 courses__item">
            <div class="courses__wapper">
                <div class="col-equal courses__top">
                    <div class="courses__top__wapper"><img class="courses__top__image"
                            src="assets/img/courses-image/0.jpg" alt=""></div>
                    <div class="courses__top__bundle"></div>
                    <div class="courses__top__bt-seller"></div>
                    <div class="courses__hover">
                        <div class="courses__hover-content"><span class="courses__hover-content__item"><span
                                    class="glyph-icon flaticon-shapes courses__hover-content__icon"></span></span><span
                                class="courses__hover-content__item"><span
                                    class="glyph-icon flaticon-interface courses__hover-content__icon"></span></span>
                        </div>
                    </div>
                </div>
                <div class="col-equal courses__content"><a class="courses__content__title" href="#">
                    {{u.nom}} {{ u.prenom}}
                </a>
                    <div class="courses__content__author"><a class="courses__col-left" href="#"> <span
                                class="courses__content__author-name">{{u.adresseEmail}}</span></a>
                         
                    </div>
                     
                </div>
            </div>
        </div> 
        {% endif %}
        {% endfor %}




    </div>  
    </div>   
</div>
        

{% endblock %}