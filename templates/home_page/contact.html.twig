{% extends 'baseHeaderFooter/header.html.twig' %}

{% block title %}Hello Home{% endblock %}

{% block body %}

<div class="sc-contact-page">
    <div class="container">
        <div class="contact-page__wrapper">
            <div class="cotact-page__box">
                <div class="row">
                    <div class="col-md-8">
                        <div class="contact-page__box__left">
                            <div class="contact-header-l">
                                <h4 class="title-text">Just Say Hi !</h4>
                                <p class="content-text">Ad aperiam aspernatur atque aut consequatur distinctio earum ex
                                    fuga fugit hic impedit iure libero minima molestiae nihil obcaecati officia
                                    perferendis.</p>
                            </div>
                            <div class="contact-form-l">
                                <form class="contact-form" action="#">
                                    <div class="contact-form__group-name">
                                        <div class="row">
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="sr-only" for="firstName"></label>
                                                    <input class="form-control" type="text" placeholder="First Name"
                                                        id="firstName">
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="sr-only" for="lastName"></label>
                                                    <input class="form-control" type="text" placeholder="Last Name"
                                                        id="lastName">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="contact-form__group-contact">
                                        <div class="row">
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="sr-only" for="email"></label>
                                                    <input class="form-control" type="email" placeholder="Your Email"
                                                        id="email">
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="sr-only" for="phone"></label>
                                                    <input class="form-control" type="text" placeholder="Phone Number"
                                                        id="phone">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="contact-form__mess">
                                        <textarea class="form-control"
                                            placeholder="Everybody good? Plenty of slaves for my robot colony? - Interstellar"
                                            rows="5"></textarea>
                                    </div>
                                    <button class="btn button-normal-02 contact-form__button"
                                        type="submit">SUBMIT</button>
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="contact-page__box__right">
                            <div class="contact-information-r">
                                <h4 class="title-text">Contact Information</h4>
                                <div class="contact-information__wrapper">
                                    <ul class="contact-information">
                                        <li> <span class="icon-custom icon-contact flaticon-tool-1"></span><span>(+972
                                                2) 2365 629 989</span></li>
                                        <li> <span
                                                class="icon-custom icon-contact flaticon-interface-2"></span><span>hello@educef.com</span>
                                        </li>
                                        <li> <span class="icon-custom icon-contact flaticon-clock"></span><span>9:00 AM
                                                - 7:00 PM<br class="hidden-desktop">( Monday - Friday )</span></li>
                                        <li><span
                                                class="icon-custom icon-contact flaticon-facebook-placeholder-for-locate-places-on-maps"></span><span>|
                                                1735 Vine St.Hollywood CA 900</span></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="contact-connect-r">
                                <h4 class="title-text">Connect</h4>
                                <div class="contact-connect__wrapper">
                                    <ul class="contact-connect">
                                        <li class="contact-connect__item"><a class="contact-connect__link"
                                                href="#"><span
                                                    class="icon-custom icon-connect flaticon-social-media"></span></a>
                                        </li>
                                        <li class="contact-connect__item"><a class="contact-connect__link"
                                                href="#"><span
                                                    class="icon-custom icon-connect flaticon-social-1"></span></a></li>
                                        <li class="contact-connect__item"><a class="contact-connect__link"
                                                href="#"><span
                                                    class="icon-custom icon-connect flaticon-twitter-logo-silhouette"></span></a>
                                        </li>
                                        <li class="contact-connect__item"><a class="contact-connect__link"
                                                href="#"><span
                                                    class="icon-custom icon-connect flaticon-google-plus-symbol"></span></a>
                                        </li>
                                        <li class="contact-connect__item"><a class="contact-connect__link"
                                                href="#"><span
                                                    class="icon-custom icon-connect flaticon-social-4"></span></a></li>
                                        <li class="contact-connect__item"><a class="contact-connect__link"
                                                href="#"><span
                                                    class="icon-custom icon-connect flaticon-pinterest-logotype"></span></a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="contact-page__breadcrumb">
            <div class="breadcrumb-wapper">
                <ul class="breadcrumb">
                    <li class="breadcrumb__item"><a class="breadcrumb__link" href="#">Home</a></li>
                    <li class="breadcrumb__item active"><a class="breadcrumb__link active" href="#">Contact us</a></li>
                </ul>
            </div>
        </div>
    </div>
</div>

{% endblock %}